vec($_=<>.v128,
	($/ *= ~-y---c) / 64 + 1 | 7,
	@; = map { __(.5) } @p = grep { (1x$_) !~ /^(11+)\1+$/ } 2..311
) = $/ = 8,

@; = map {

sub'__() { $! = ($k = 2**32) * $_ ** pop, or
		   $j = $;[105-ord$&x+beedecyighighpxy=~/./g.k] }
sub'h { __; map $! ^= $j + $j*$k*!/\d/ >> -5-ord, pop=~/./g; $! }

	$z = 0,
	@; = map { ($`[$z++] += $_) %= $k } @;, unpack-_^cu, $_;
	$;[3] += $h = &__(1/3) + __ + h(upb) + (__&__|~__&__) + __,
	@; = map { $_ % $k } $h + h(yne) + (__&__|__&(__|__)), @;,
					  __ + h(ti8) + __ + h(jh1) for @p
} /.{64}|/sg;

print $k, $", (2**37+1) % $k, v10;
printf '%08x'x8, @`

